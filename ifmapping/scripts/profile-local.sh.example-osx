GEOUTILS_LIB_PATH=$IFCG_SVN_PATH/geoutils/lib
GEOUTILS_PYTHONPATH=$IFCG_SVN_PATH/geoutils/swig/python
TINYXML_LIB_PATH=$FLUX_USR/lib
DEPS_PYTHONPATH=$FLUX_SVN/stuff/trunk/ifmapping/deps/python
