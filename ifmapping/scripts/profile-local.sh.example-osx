PYTHONPATH=$PYTHONPATH:$IFMAPPING_PYTHONPATH:$IFOBJECT_PYTHONPATH:$ALTJIRA_PYTHONPATH:$GEOUTILS_PYTHONPATH:$DEPS_PYTHONPATH:./python
export PATH LD_LIBRARY_PATH PYTHONPATH
